            <block class="Magento\Framework\View\Element\Template" template="StudioEmma_Optimus::content/layout/columns-maxwidth.phtml"/>
            <block class="Magento\Framework\View\Element\Template" template="StudioEmma_Optimus::content/layout/centered-icon.phtml"/>
            <block class="Magento\Framework\View\Element\Template" template="StudioEmma_Optimus::content/layout/pagewide-invert.phtml"/>
